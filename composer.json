{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": [
        "framework",
        "laravel",
        "lumen"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "ext-json": "*",
        "flipbox/lumen-generator": "^5.6",
        "ghostff/lumen-form-request-php7": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/notifications": "^5.8",
        "laravel/lumen-framework": "5.8.*",
        "maxsky/lumen-app-key-generator": "^1.0",
        "palanik/lumen-cors": "dev-master",
        "paypal/rest-api-sdk-php": "^1.14",
        "prettus/l5-repository": "^2.6",
        "ramsey/uuid": "^3.8",
        "sayeed/custom-migrate": "^1.0",
        "tymon/jwt-auth": "^1.0.0-rc.1",
        "vlucas/phpdotenv": "^3.3"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.7",
        "fzaninotto/faker": "^1.4",
        "league/flysystem": "^1.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classics": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ],
        "files": [
            "app/Libraries/Constants/Constants.php",
            "app/Libraries/Constants/helpers.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
